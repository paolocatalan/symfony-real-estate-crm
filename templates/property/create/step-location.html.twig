{% extends 'base.html.twig' %}

{% block title %}Add the property location{% endblock %}

{% block body %}
    <h1>Add the property location</h1>
    {{ form_start(form) }}
    {{ form_row(form.address) }}
    {{ form_row(form.city) }}
    {{ form_row(form.state) }}
    {{ form_row(form.zip_code) }}
    {{ form_row(form.country) }}
    <button type="submit">Next</button>
    {{ form_end(form) }}

{% endblock %}
