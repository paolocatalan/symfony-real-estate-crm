{% extends 'base.html.twig' %}

{% block title %}Add Property Step 1{% endblock %}

{% block body %}
    <h1>Add Property Step 1</h1>

    {{ form_start(form) }}
    {{ form_row(form.address) }}
    {{ form_row(form.city) }}
    {{ form_row(form.state) }}
    {{ form_row(form.zip_code) }}
    {{ form_row(form.country) }}
    <button type="submit">Submit Property</button>
    {{ form_end(form) }}

{% endblock %}
