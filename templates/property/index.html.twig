{% extends 'base.html.twig' %}

{% block title %}Property Listings{% endblock %}

{% block body %}
    <h1>Property Listings</h1>
    <ul>
        {% for property in properties %}
            <li><a href="{{ path('show_property', {id: property.id}) }}">{{ property.address }}</a></li>
        {% endfor %}
    </ul>
{% endblock %}
